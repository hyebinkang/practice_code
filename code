      "cell_type": "code",
      "source": [
        "t= int(input())\n",
        "\n",
        "for _ in range(t):\n",
        "  palindrome = 0\n",
        "  a, b= map(int, input().split())\n",
        "\n",
        "  for i in range(a, b+1):\n",
        "    c = i**(1/2)                          #제곱수\n",
        "    if c == int(c):                       #제곱수가 int이면\n",
        "      i = str(i)                          \n",
        "      c= str(int(c))                      #i를 문자로 변환, c를 문자로 변환\n",
        "\n",
        "      if i == i[::-1] and c == c[::-1]:   #i와 i의 뒷자리, c와 c의 뒷자리 부터 비교했을 때 같다면 +1\n",
        "        palindrome += 1\n",
        "\n",
        "  print(\"#{0}\".format(_+1), palindrome)\n"
