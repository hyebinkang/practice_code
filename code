        "t = int(input())\n",
        "alphabet = 'abcdefghijklmnopqrstuvwxyz'               #알파벳 문자 순서대로\n",
        "\n",
        "for _ in range(t):\n",
        "  count = 0\n",
        "  word = [0 for i in range(26)]                       #리스트에 모두 0으로 채워줌\n",
        "\n",
        "  s = input()\n",
        "  for i in range(len(s)):                             #s의 길이만큼 0을 s의 문자로 채움\n",
        "    word[i] = s[i]\n",
        "\n",
        "  \n",
        "  for i in range(26):\n",
        "    if alphabet[i] == word[i]:                       #i번째 오는 알파벳이 같으면 +1\n",
        "      count +=1\n",
        "    else:\n",
        "      break                                         #같지 않으면 끝내기\n",
        "\n",
        "  print(\"#{0}\".format(_+1), count)"
      ]
    },
